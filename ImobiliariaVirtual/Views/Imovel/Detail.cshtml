@model Imovel

    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Detalhes do Imóvel</h2>
            <hr />
        </div>
        @*<div asp-validation-summary="All"></div>*@
        <div class="mb-3">
            <label asp-for="Tipo"></label>
            <input asp-for="Tipo" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="ValorVenda"></label>
            <input asp-for="ValorVenda" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="ValorLocacao"></label>
            <input asp-for="ValorLocacao" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="Endereco"></label>
            <input asp-for="Endereco" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="Numero"></label>
            <input asp-for="Numero" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="Complemento"></label>
            <input asp-for="Complemento" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="Bairro"></label>
            <input asp-for="Bairro" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="Cidade"></label>
            <input asp-for="Cidade" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="Estado"></label>
            <input asp-for="Estado" disabled class="form-control" />
        </div>
        <div class="mb-3">
            <label asp-for="CEP"></label>
            <input asp-for="CEP" disabled class="form-control" />
        </div>
        <a asp-controller="Imovel" asp-action="Index" class="btn btn-secondary" style="width:150px">
            Voltar para lista
        </a>
    </div>